@model IEnumerable<TweedeZit.Models.Domain.Leertraject>

<table class="table table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Titel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Omschrijving)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Titel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Omschrijving)
        </td>
        <td>
            <a href="@Url.Action("Opleidingsdagen", new { id=item.LeertrajectId })"><span class="badge badge-info">Toon trainingsdagen</span></a>
            <a href="@Url.Action("IndexZwemmers", new { id=item.LeertrajectId })"><span class="badge badge-info">Toon zwemmers</span></a>
            <a href="@Url.Action("Edit", new { id=item.LeertrajectId })"><span class="badge badge-info">Wijzig groep</span></a>
        </td>
    </tr>
}

</table>

<p>&nbsp;</p>